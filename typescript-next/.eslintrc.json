{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "max-lines": ["error", { "max": 200, "skipBlankLines": true }],
        "max-lines-per-function": [
            "error",
            { "max": 75, "skipBlankLines": true, "skipComments": true }
        ],
        "complexity": ["error", 6]
    }
}